{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "securitySettings": {
    "tokenKey": "@(encryptionKey)",
    "tokenLifeMinutes": 1
  },
  "TotpSettings": {
    "totpSecretKey":"@(totpSecretKey)",
    "totpTokenLifetimeInSeconds": 300
  },
  "AirApiSettings": {
    "token":"@(AirApiToken)",
    "url":"https://api.travelpayouts.com/v2/prices/latest"
  }
}
